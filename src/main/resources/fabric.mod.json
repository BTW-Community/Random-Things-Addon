{
  "schemaVersion": 1,
  "id": "randomthings",
  "version": "${version}",

  "name": "Random Things",
  "description": "This addon adds random features, including a timer for speedrunning and three keybinds for changing the camera placement instead of F5",
  "authors": [
    "Umngane Wami"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/randomthings/icon.png",

  "environment": "*",
  "entrypoints": {
    "init": [
      "net.random.things.RandomThingsMod"
    ],
    "preLaunch": [
      "net.random.things.RandomThingsPreLaunchInitializer"
    ]
  },
  "mixins": [
    "randomthings.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4"
  },
  "suggests": {
    "flamingo": "*"
  }
}
